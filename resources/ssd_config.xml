<?xml version="1.0" encoding="UTF-8"?>

<root>
	<physical>
		<!-- OP as percents -->
		<overprovisioning>30</overprovisioning>
		<!-- GCT is garbage collection threshold - as percents -->
		<gc_threshold></gc_threshold>
		<gc_threshold_blocks>6</gc_threshold_blocks>
		<chips>5</chips>
		<planes>1</planes>
		<blocks>32</blocks>
		<pages>24</pages>
		<max_erasures>100000</max_erasures>
	</physical>
	<visual>
		<view_sample>1</view_sample>
		<show_counters>yes</show_counters>
		<speed>60000</speed><!-- Number of frames per minute -->
		<page_width>18</page_width>
		<page_height>11</page_height>
		<block_space>1</block_space><!-- spacing between the blocks in the plane -->
		<pages_in_row>8</pages_in_row>
		<blocks_in_row>2</blocks_in_row>
		<planes_in_row>1</planes_in_row>
		<show_old_parity>yes</show_old_parity>
		<show_old_data>no</show_old_data>
		<font_type>Segoe UI</font_type>
		<caption_font_size>14</caption_font_size>
		<control_font_size>12</control_font_size>
		<page_font_size>9</page_font_size>
		<active_color r="255" g="0" b="0" />
		<outer_bg_color r="40" g="40" b="40" />
		<intermediate_bg_color r="50" g="50" b="50" />
		<inner_bg_color r="64" g="64" b="64" />
		<border_color r="128" g="128" b="128" />		
		<page_text_color r="0" g="0" b="0" />
		<control_text_color r="192" g="192" b="192" />
		<highlight_color r="85" g="85" b="85" />
	</visual>
	<ReusableVisualizationSSDManager>
		<name>Reusable Simulation</name>
		<clean_color r="255" g="255" b="255" />
		<first_write r="50" g="255" b="150" />
		<second_write r="50" g="150" b="255" />
	</ReusableVisualizationSSDManager>
	<ReusableSSDManager>
		<name>Reusable</name>
		<clean_color r="255" g="255" b="255" />
		<first_write r="50" g="255" b="150" />
		<second_write r="50" g="150" b="255" />
	</ReusableSSDManager>
	<HotColdReusableSSDManager>
		<name>HotCold-Reusable</name>
		<temp_limit>5</temp_limit>
		<clean_color r="255" g="255" b="255" />
		<first_write r="50" g="255" b="150" />
		<second_write r="50" g="150" b="255" />
	</HotColdReusableSSDManager>
	<HotColdSSDManager>
		<name>HotCold</name>
		<clean_color r="255" g="255" b="255" />
		<intermediate r="200" g="200" b="200" />
		<cold r="255" g="60" b="60" />
		<hot r="60" g="120" b="255" />
		<min_temperature>1</min_temperature>
		<max_temperature>10</max_temperature>
		<partition>1</partition>
		<partition>5</partition>
		<partition>10</partition>
	</HotColdSSDManager>
	<GreedySSDManager>
		<name>Greedy</name>
		<clean_color r="255" g="255" b="255" />
		<written_color r="255" g="220" b="100" />
	</GreedySSDManager>
	<RAIDVisualizationSSDManager>
		<name>RAID Simulation</name>
		<clean_color r="255" g="255" b="255"></clean_color>
		<parity_color r="150" g="150" b="150"></parity_color>
		<parity_color r="200" g="100" b="30"></parity_color>
		<data_color r="255" g="220" b="100"></data_color>
		<stripe_size>3</stripe_size>
		<stripe_frame_color r="255" g="0" b="0"></stripe_frame_color>
		<stripe_frame_step r="140" g="64" b="32"></stripe_frame_step>
	</RAIDVisualizationSSDManager>
	<RAID1SSDManager>
		<name>RAID 1</name>
		<clean_color r="255" g="255" b="255"></clean_color>
		<parity_color r="150" g="150" b="150"></parity_color>
		<data_color r="255" g="220" b="100"></data_color>
		<stripe_frame_color r="225" g="0" b="0"></stripe_frame_color>
		<stripe_frame_step r="140" g="64" b="32"></stripe_frame_step>
	</RAID1SSDManager>
	<RAID5SeparateSSDManager>
		<name>RAID 5 Separate Parity</name>
		<clean_color r="255" g="255" b="255"></clean_color>
		<parity_color r="150" g="150" b="150"></parity_color>
		<data_color r="255" g="220" b="100"></data_color>
		<stripe_frame_color r="225" g="0" b="0"></stripe_frame_color>
		<stripe_frame_step r="140" g="64" b="32"></stripe_frame_step>
	</RAID5SeparateSSDManager>
	<RAID5SSDManager>
		<name>RAID 5</name>
		<clean_color r="255" g="255" b="255"></clean_color>
		<parity_color r="150" g="150" b="150"></parity_color>
		<data_color r="255" g="220" b="100"></data_color>
		<stripe_frame_color r="225" g="0" b="0"></stripe_frame_color>
		<stripe_frame_step r="140" g="64" b="32"></stripe_frame_step>
	</RAID5SSDManager>
	<RAID6SSDManager>
		<name>RAID 6</name>
		<clean_color r="255" g="255" b="255"></clean_color>
		<parity_color r="150" g="150" b="150"></parity_color>
		<parity_color r="50" g="100" b="200"></parity_color>
		<data_color r="255" g="220" b="100"></data_color>
		<stripe_frame_color r="225" g="0" b="0"></stripe_frame_color>
		<stripe_frame_step r="140" g="64" b="32"></stripe_frame_step>
	</RAID6SSDManager>
</root>
